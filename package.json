    "deep-equal": "^1.0.1",
    "events-polyfill": "^1.1.9",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "ng2-pdf-viewer": "^5.1.1",
    "ngx-bootstrap": "^2.0.5",
    "ngx-clipboard": "^11.1.9",
    "node": "^11.15.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "tabbable": "^1.1.2",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.26"
