    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/angular": "^5.2.1",
    "@storybook/cli": "^5.2.1",
    "@swimlane/prettier-config-swimlane": "^2.0.0",
    "@types/d3-array": "^1.2.1",
