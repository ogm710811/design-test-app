{
  "name": "fox-ui-client",
  "version": "1.0.0",
  "repository": "https://github.optum.com/bff-services/fox-ui.git",
  "scripts": {
    "ng": "./node_modules/@angular/cli/bin/ng",
    "csslint": "./node_modules/csslint/dist/cli.js --warnings=zero-units,order-alphabetical ./src/assets/site.css",
    "htmlhint": "./node_modules/.bin/htmlhint -c .htmlhintrc src",
    "lint": "./node_modules/tslint/bin/tslint -p ./src/tsconfig.app.json --exclude **/swagger-generated/**/* --exclude **/coverage/**/*",
    "test": "ng t --passWithNoTests",
    "prebuild-parallel": "echo 'Start Build:' `date`",
    "build-parallel": "npm run lint > lint.log 2>&1 & npm run csslint > csslint.log 2>&1 & npm run htmlhint > htmlhint.log 2>&1 & npm run test > test.log 2>&1 & npm run e2e > e2e.log 2>&1 & npm run build-prod > build-prod.log 2>&1",
    "postbuild-parallel": "cat lint.log && cat csslint.log && cat htmlhint.log && cat test.log && cat e2e.log && cat build-prod.log && echo 'Build Complete:' `date`",
    "build": "./build.sh",
    "watch-dev": "./node_modules/@angular/cli/bin/ng b --watch",
    "prebuild-state-management": "ng l state-management",
    "build-state-management": "ng b state-management",
    "postbuild-state-management": "ng t state-management --passWithNoTests",
    "prebuild-rest-clients": "npm run build-state-management && ng l rest-clients",
    "build-rest-clients": "ng b rest-clients",
    "postbuild-rest-clients": "ng t rest-clients --passWithNoTests",
    "prebuild-test-support": "npm run build-rest-clients && ng l test-support",
    "build-test-support": "ng b test-support",
    "prebuild-shared": "npm run build-test-support && ng l shared",
    "build-shared": "ng b shared",
    "postbuild-shared": "ng t shared --passWithNoTests",
    "prebuild-file-maintenance": "npm run build-shared",
    "build-file-maintenance": "ng b file-maintenance",
    "prebuild-login": "npm run build-file-maintenance",
    "build-login": "ng b login",
    "build-prod": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng b --prod=true",
    "prebuild-dev": "npm run build-shared",
    "build-dev": "./node_modules/@angular/cli/bin/ng b",
    "postbuild-dev": "ng t fox-ui-client --passWithNoTests",
    "preneeds-state-management": "ng b state-management",
    "needs-state-management": "ng t state-management --passWithNoTests",
    "preneeds-rest-clients": "npm run needs-state-management",
    "needs-rest-clients": "ng t rest-clients --passWithNoTests & ng b test-support & wait",
    "preneeds-test-support": "npm run needs-rest-clients",
    "needs-test-support": "ng b shared",
    "preneeds-shared": "npm run needs-test-support",
    "needs-shared": "ng t shared --passWithNoTests & ng b login & wait",
    "preneeds-feature-modules": "npm run needs-shared",
    "test-feature-modules": "ng t login --passWithNoTests",
    "needs-feature-modules": "ng t fox-ui-client --passWithNoTests & npm run test-feature-modules & npm run e2e & npm run build-prod & wait",
    "start-mock": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --configuration=mock --host 0.0.0.0",
    "start-dev": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-in1.conf.json --port 4211 --host 0.0.0.0",
    "start-int": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-in1.conf.json --port 4211 --host 0.0.0.0",
    "start-in1": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-in1.conf.json --port 4211 --host 0.0.0.0",
    "start-in2": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-in2.conf.json --port 4212 --host 0.0.0.0",
    "start-in3": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-in3.conf.json --port 4213 --host 0.0.0.0",
    "start-in4": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-in4.conf.json --port 4214 --host 0.0.0.0",
    "start-tst": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-ts1.conf.json --port 4221 --host 0.0.0.0",
    "start-qa": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-qa1.conf.json --port 4231 --host 0.0.0.0",
    "start-qa1": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-qa1.conf.json --port 4231 --host 0.0.0.0",
    "start-qa2": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-qa2.conf.json --port 4232 --host 0.0.0.0",
    "start-prod": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve -prod --proxy-config proxy-prod.conf.json --host 0.0.0.0",
    "pree2e-ci": "./node_modules/protractor/bin/webdriver-manager update --ignore_ssl",
    "e2e-ci": "./node_modules/@angular/cli/bin/ng e2e --protractor-config protractor-mock.conf.js --configuration=mock --port 4299",
    "e2e-spring": "./node_modules/@angular/cli/bin/ng e2e --protractor-config protractor-remote.conf.js --no-serve --base-href http://localhost:8080/#/",
    "e2e-node": "./node_modules/@angular/cli/bin/ng e2e --protractor-config protractor-remote.conf.js --no-serve --base-href http://localhost:4200/#/ --proxy-config proxy-in1.conf.json",
    "e2e-npm": "npm run e2e-node",
    "pree2e-int": "./node_modules/protractor/bin/webdriver-manager update --ignore_ssl",
    "e2e-int": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng e2e --protractor-config protractor-remote.conf.js --configuration=remote",
    "e2e": "npm run e2e-ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "types": "./src/main.ts",
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^7.0.0",
