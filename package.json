    "@ng-idle/core": "^6.0.0-beta.3",
    "@ng-idle/keepalive": "^6.0.0-beta.3",
    "@ng-select/ng-select": "^2.16.4",
    "@ngrx/effects": "^7.0.0-beta.0",
    "@ngrx/store": "^7.0.0-beta.0",
    "@swimlane/ngx-charts": "^6.1.0",
    "@swimlane/ngx-ui": "^17.2.9",
    "@types/core-js": "^0.9.43",
    "@types/lodash": "^4.14.144",
    "@types/uuid": "^3.4.3",
    "angular-bootstrap-md": "^6.1.1",
