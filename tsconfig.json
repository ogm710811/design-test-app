{
  "compilerOptions": {
    "outDir": "../out-tsc/",
    "module": "es2015",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "src/app/**/*.d.ts"
    ],
    "lib": [
      "es2016",
      "es2017.object",
      "dom"
    ],
    "baseUrl": "./",
    "paths": {
      "@fox/*": [
        "dist/*"
      ]
    }
  }
}